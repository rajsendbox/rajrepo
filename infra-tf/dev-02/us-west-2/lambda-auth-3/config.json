{
  "function_name": "Authenticator-api-gateway",
  "description": "This is lambda for api-gateway authenticator",
  "handler": "lambda_function.lambda_handler",
  "runtime": "python3.11",
  "event_source_arn": "",
  "source_arn": "",
  "create_event_source_mapping": false,
  "create_allowed_triggers": false,
  "use_local_package": true,
  "use_image_uri": false,
  "use_s3_bucket": false,
  "local_existing_package": "app.zip",
  "lambda_layer_arns":["arn:aws:lambda:us-west-2:637423201724:layer:requests:5"],
  "timeout": 300,
  "architectures": ["x86_64"],
  "environment_variables":{
  "AUTH_API_INTERNAL_ENDPOINT": "http://identra-nlb-741ea54ecf348913.elb.us-west-2.amazonaws.com:8002",
  "KEYCLOAK_API_INTERNAL_ENDPOINT": "http://identra-nlb-741ea54ecf348913.elb.us-west-2.amazonaws.com:8001",
  "KEYCLOAK_CREDS": "keyclock/admin/creds"
  },
  "subnet": ["subnet-0519c1c57ce033d5b","subnet-086e6bfe4e2c26b40"],
  "security_group": ["sg-00528894045b3e4f8"],
  "policy_json": {
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Action": [
          "secretsmanager:GetRandomPassword",
          "secretsmanager:GetResourcePolicy",
          "secretsmanager:GetSecretValue",
          "secretsmanager:DescribeSecret",
          "secretsmanager:ListSecretVersionIds",
          "secretsmanager:ListSecrets",
          "secretsmanager:BatchGetSecretValue",
          "ec2:*"
        ],
        "Resource": "*"
      }
    ]
